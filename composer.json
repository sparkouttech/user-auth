{
    "name": "sparkouttech/user-auth",
    "description": "User authentication package",
    "keywords": [
        "sparkouttech",
        "user-auth"
    ],
    "homepage": "https://github.com/sparkouttech/user-auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "sivabharathy",
            "email": "gotocva@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ramsey/uuid": "^4.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sparkouttech\\UserAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sparkouttech\\UserAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sparkouttech\\UserAuth\\UserAuthServiceProvider"
            ],
            "aliases": {
                "UserAuth": "Sparkouttech\\UserAuth\\UserAuthFacade"
            }
        }
    }
}
